{
  "name": "gap",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock",
    "clean:install": "yarn run clean && yarn install",
    "dev": "next dev",
    "build": "next build",
    "build:image": "npm run build && docker build -t gap-web-docker .",
    "test": "jest --watch",
    "format": "prettier --write .",
    "coverage": "jest --coverage",
    "start": "NODE_OPTIONS='-r next-logger/presets/next-only' next start",
    "lint": "next lint",
    "postinstall": "node -e \"try { import('husky').then(i => i.default()); } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "integration": "yarn cypress run",
    "integration:gui": "yarn run cypress open",
    "contentful-typescript-codegen": "contentful-typescript-codegen --output @types/generated/contentful.d.ts"
  },
  "dependencies": {
    "@aws-crypto/client-node": "^3.2.0",
    "@contentful/rich-text-react-renderer": "^15.11.1",
    "@elastic/elasticsearch": "7.10",
    "axios": "1.6.8",
    "body-parser": "^1.20.2",
    "contentful": "^9.1.5",
    "cookie-parser": "^1.4.6",
    "cypress-recurse": "^1.18.0",
    "dotenv": "^16.0.0",
    "govuk-frontend": "^3.15",
    "imap-simple": "^5.1.0",
    "jose": "^5.2.4",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.6.7",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "next": "13.5.6",
    "next-logger": "4.0.0",
    "nodemailer": "^6.9.9",
    "nookies": "^2.5.2",
    "notifications-node-client": "^8.0.0",
    "pg": "^8.7.3",
    "pino": "8.20.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-moment": "^1.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@elastic/elasticsearch-mock": "^1.0.0",
    "@swc/core": "^1.3.104",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "contentful-management": "^10.0.0",
    "contentful-typescript-codegen": "^3.2.3",
    "cypress": "^13.2.0",
    "cypress-axe": "^1.5.0",
    "cypress-postgres": "^1.1.1",
    "eslint": "^8.16.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "sass": "^1.50.1",
    "typescript": "^5.4.5",
    "urlpattern-polyfill": "^10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "http-cache-semantics": "^4.1.1",
    "loader-utils": "^3.2.1",
    "postcss": "^8.4.38",
    "semver": "^7.6.0",
    "ua-parser-js": "^1.0.37",
    "xml2js": "^0.6.2"
  }
}
