    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-moment": "^1.1.1",
    "react-number-format": "^4.9.1",
    "swr": "^0.5.5"
