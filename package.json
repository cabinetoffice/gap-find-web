    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.27.2",
    "body-parser": "^1.19.2",
    "contentful": "^9.1.5",
    "cypress-recurse": "^1.18.0",
    "date-fns": "^2.19.0",
    "dotenv": "^16.0.0",
    "govuk-frontend": "^3.14.0",
    "gray-matter": "4.0.3",
    "imap-simple": "^5.1.0",
    "jsonwebtoken": "9.0.0",
    "lint-staged": "^12.3.8",
    "mailparser": "^3.4.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "next": "12",
