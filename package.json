    "eslint": "^8.16.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^7.0.4",
    "jest": "26",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "3.0.3",
    "sass": "^1.50.1",
    "typescript": "^4.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
